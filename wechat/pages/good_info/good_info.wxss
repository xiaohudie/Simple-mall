.page-section{
  width: 100%;
  margin-bottom: 60rpx;
}

.page-section-spacing{
  box-sizing: border-box;
}

.page-section-spacing .swiper-section{
  width: 100vw;
  height:100vw;
}

.page-section-spacing .swiper-section .slide-image{
  width: 100%;
  height:100%;
}

.good_info{
  padding: 0 5vw;
  overflow: hidden;
}

.good_info .money_icon{
  font-size: 12px;
  color:#ff0068;

}

.good_info .money_price{
  color:#ff0068;
}

.good_info .money_icon{
  font-size: 18px;
  color:#ff0068;
}

.good_info .good_des{
  font-size: 12px;
}

.good_type{
  margin: 20px 0;
}

.good_title{
  margin: 20px 0;
  display:block;
  text-align:center;
}

.good_type .good_type_title{
  font-size: 15px;
  font-weight: normal;
  display: block;
}

.good_type .type_info{
  font-size: 12px;
  font-weight: normal;
  margin: 10px;
}

.good_text{
  font-size: 12px;
}

.good_img .good_image{
  width: 100%;
}

.toMore{
  position: fixed;
  right:5vw;
  bottom:29vh;
}

.toMore .baseIcon{
  right:5vw;
  bottom:29vh;
}

.toMore .img{
  width: 32px;
  height:32px;
  border-radius: 50%;
  box-shadow:0px 0px 3px 3px #1AAD19;
}

.toMore .moreIcon{
  position: fixed;
  right:5vw;
  bottom:29vh;
}

.toMore .moreIcon.icon1{
  width: 32px;
  height:32px;
  right:18vw;
  animation:myShow .7s ease-out;
}

.toMore .moreIcon.icon1 .img{
  display: block;
  animation:myShow1 .7s ease-in;
}

@keyframes myShow
{
  0%{

    transform: translateX(50px)
    
    }
    
    100%{
    
    transform: translateX(0px)
    
    }
 
}

@keyframes myShow1
{
  0%{

    transform: translateY(50px)
    
    }
    
    100%{
    
    transform: translateY(0px)
    
    }
 
}

.toMore .moreIcon.icon2{
  bottom:58vw;
  right:9vw;
  animation:myShow .5s ease-out;
  width: 32px;
  height:32px;
}

.toMore .moreIcon.icon2 .img{
  display: block;
  animation:myShow1 .5s ease-in;
}

.toMore .moreIcon.icon3{
  bottom:35vw;
  right:9vw;
  animation:myShowLast 1s ease-out;
  width: 32px;
  height:32px;
}

.toMore .moreIcon.icon3 .img{
  display: block;
  animation:myShowLast1 1s ease-in;
}

@keyframes myShowLast
{
  0%{

    transform: translateX(50px)
    
    }
    
    100%{
    
    transform: translateX(0px)
    
    }
 
}

@keyframes myShowLast1
{
  0%{

    transform: translateY(50px)
    
    }
    
    100%{
    
    transform: translateY(0px)
    
    }
 
}

.baseIcon.isOpen{
  transform: rotate(45deg);
  transition:all .5s;
}

.baseIcon{
  transform: rotate(0deg);
  transition:all .5s;
}